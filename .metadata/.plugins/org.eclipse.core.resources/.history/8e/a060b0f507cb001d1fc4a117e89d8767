
public class array1 {
	public static void main(String args[]) {
		int[] arr;// Array declaration
		arr = new int[5];// Giving the size an array
		
		arr[0]=10;
		arr[1]=25;
		arr[2]=22;
		arr[3]=30;
		arr[4]=26;
		
		for(int i = 0;i<arr.length;i++) {
			System.out.println("Element at index "+ i +" is "+arr[i]);
		}
		
		
 	}
}
